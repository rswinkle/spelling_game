# GNU Make workspace makefile autogenerated by Premake

ifndef config
  config=debug
endif

ifndef verbose
  SILENT = @
endif

ifeq ($(config),debug)
  spelling_game_config = debug
endif
ifeq ($(config),release)
  spelling_game_config = release
endif

PROJECTS := spelling_game

.PHONY: all clean help $(PROJECTS) 

all: $(PROJECTS)

spelling_game:
ifneq (,$(spelling_game_config))
	@echo "==== Building spelling_game ($(spelling_game_config)) ===="
	@${MAKE} --no-print-directory -C . -f spelling_game.make config=$(spelling_game_config)
endif

clean:
	@${MAKE} --no-print-directory -C . -f spelling_game.make clean

help:
	@echo "Usage: make [config=name] [target]"
	@echo ""
	@echo "CONFIGURATIONS:"
	@echo "  debug"
	@echo "  release"
	@echo ""
	@echo "TARGETS:"
	@echo "   all (default)"
	@echo "   clean"
	@echo "   spelling_game"
	@echo ""
	@echo "For more information, see https://github.com/premake/premake-core/wiki"